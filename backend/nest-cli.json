{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "monorepo": true,
  "root": "apps/gateway",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "userSvc": {
      "type": "application",
      "root": "apps/userSvc",
      "entryFile": "main",
      "sourceRoot": "apps/userSvc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/userSvc/tsconfig.app.json"
      }
    },
    "sneakerSvc": {
      "type": "application",
      "root": "apps/sneakerSvc",
      "entryFile": "main",
      "sourceRoot": "apps/sneakerSvc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sneakerSvc/tsconfig.app.json"
      }
    },
    "brandSvc": {
      "type": "application",
      "root": "apps/brandSvc",
      "entryFile": "main",
      "sourceRoot": "apps/brandSvc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/brandSvc/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "image-processing": {
      "type": "application",
      "root": "apps/image-processing",
      "entryFile": "main",
      "sourceRoot": "apps/image-processing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/image-processing/tsconfig.app.json"
      }
    }
  }
}
